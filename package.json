{
    "name": "svm-js",
    "version": "1.0.0",
    "description": "Simple Browser Implementation of Support Vector Machine",
    "main": "dist/index.js",
    "repository": "https://github.com/Garg-Doppler/SVM",
    "author": "Garg-Doppler",
    "license": "MIT",
    "scripts": {
        "build-web": "browserify src/index.js -o dist/index.js -t [ babelify --presets [ @babel/preset-env ] ]",
        "build-cli": "browserify src/index.js --node -o dist/index.js -t [ babelify --presets [ @babel/preset-env ] ]",
        "start": "yarn build-cli && node dist/index.js"
    },
    "dependencies": {
        "@babel/cli": "^7.10.4",
        "@babel/core": "^7.10.4",
        "babel-plugin-transform-object-rest-spread": "^7.10.4",
        "@babel/preset-env": "^7.10.4",
        "babelify": "^10.0.0",
        "browserify": "^16.5.1",
        "libsvm-js": "^0.2.1",
        "ml-cross-validation": "^1.3.0",
        "ml-dataset-iris": "^1.1.1",
        "ml-random-forest": "^2.0.1"
    }
}